{
  "version": 2,
  "builds": [
    {
      "src": "./public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/templates/index.html"
    },
    {
      "src": "/(.*\\.html)",
      "dest": "/public/templates/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/public/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/templates/$1"
    }
  ]
}